package exercicios_estrutura_condicional;

import java.util.Scanner;

public class exercicio_03 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int num1, num2;
		
		System.out.println("Digite dois valores inteiros:");
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		
		if (num1 >= num2) {
			if (num1 % num2 == 0) {
				System.out.printf("%d e %d são Multiplos%n", num1, num2);
			}
			else {
				System.out.printf("%d e %d NÃO são Multiplos%n", num1, num2);
			}
		}
		else {
			if (num2 % num1 == 0) {
				System.out.printf("%d e %d são Multiplos%n", num1, num2);
			}
			else {
				System.out.printf("%d e %d NÃO são Multiplos%n", num1, num2);
			}
			
		}
		
		sc.close();
	}
}
